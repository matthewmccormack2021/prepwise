# PrepWise Environment Configuration
# Copy this file to .env and fill in your API keys

# Required: Google Gemini API key for AI interview functionality
INTERVIEWER_GEMINI_API_KEY=your_gemini_api_key_here

# Optional: OpenAI API key (alternative to Gemini)
INTERVIEWER_OPENAI_API_KEY=your_openai_api_key_here

# Optional: Debug mode
INTERVIEWER_DEBUG=false

# Optional: Log level
INTERVIEWER_LOG_LEVEL=INFO

# Transcription Service Configuration
# These are used by the transcription service container
WHISPER_MODEL=base
